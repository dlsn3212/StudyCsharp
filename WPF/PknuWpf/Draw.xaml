<Window x:Class="PknuWpf.Draw"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PknuWpf"
        mc:Ignorable="d"
        Title="Draw" Height="500" Width="600">
    <StackPanel Orientation="Vertical" Width="300" >
        <Ellipse Fill="Bisque" Width="300" Height="200"/>
        <Rectangle Fill ="SlateBlue" Width="300" Height="150"/>
        <Path Height="100" Width="300" Fill="Plum">
            <Path.Data>
                <!--<RectangleGeometry Rect="100,0,100,100" />-->
                <RectangleGeometry Rect="100,0,100,100" />
            </Path.Data>
        </Path>

    </StackPanel>
</Window>
